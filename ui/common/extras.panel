
itemDef {
	name buttonhmain2
	text "Credits"
	background MP_WIDEBUTTONBG
	style 1
	type ITEM_TYPE_BUTTON
	textstyle 2
	textscale 0.5
	rect 32 204 256 32
	textalignx	 32	
	textaligny	 24
	forecolor MP_TEXTCOLOR
	action {
		play MP_SND_SELECT;
		close extras;
		open credits;
	}
	mouseEnter { setcvar ui_cdkeychecked "Contributors list!" fadein tooltip; } mouseExit { fadeout tooltip;} 
	visible 1 
}
